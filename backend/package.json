{
  "name": "happy-baby-style-admin-backend",
  "version": "1.0.0",
  "description": "Backend API for Happy Baby Style Admin Panel",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "start": "node -r tsconfig-paths/register dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:postgres": "node scripts/test-postgres-connection.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "@supabase/supabase-js": "^2.38.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.9",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  }
}
