{
  "mcpServers": {
    "awslabs.postgres-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e", "AWS_ACCESS_KEY_ID",
        "-e", "AWS_SECRET_ACCESS_KEY", 
        "-e", "AWS_REGION=us-east-2",
        "awslabs/postgres-mcp-server:latest",
        "--resource_arn", "arn:aws:rds:us-east-2:ACCOUNT_ID:db:happy-baby-style-db",
        "--secret_arn", "arn:aws:secretsmanager:us-east-2:ACCOUNT_ID:secret:rds-db-credentials/postgres",
        "--database", "happy_baby_style_db",
        "--region", "us-east-2",
        "--readonly", "true"
      ],
      "env": {
        "AWS_PROFILE": "default",
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "mcp-server-aws": {
      "command": "docker",
      "args": [
        "run",
        "-i", 
        "--rm",
        "-e", "AWS_ACCESS_KEY_ID",
        "-e", "AWS_SECRET_ACCESS_KEY",
        "-e", "AWS_REGION=us-east-2",
        "mcp-server-aws:latest"
      ],
      "env": {
        "AWS_PROFILE": "default",
        "AWS_REGION": "us-east-2"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}